import java.util.Scanner;
public class Main {
    public static int gcd(int a, int b) {
        if(b==0)
        return -1;
        while (b != 0) {
            int temp = b;
            b = a % b;
            a = temp;
        }
        return a;
    
}
    public static void main(String[] args) {

        Scanner obj = new Scanner(System.in);
        int a = obj.nextInt();
        int b = obj.nextInt();
        System.out.println(gcd(a,b));
    }
}