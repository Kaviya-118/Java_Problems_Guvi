import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String n = sc.next(); 
        long product = 1;
        for (int i = 0; i < n.length(); i++) {
            product *= (n.charAt(i) - '0');
        }
        System.out.print(product);
    }
}