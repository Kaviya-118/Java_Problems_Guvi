import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int userInput = obj.nextInt();
        int k=0;
        for(int i=2;i<=userInput/2;i++){
        if(userInput%i==0){
        k=1;
        break;}
        }
        if(k==1 ||userInput<2 )
        System.out.print("no");
        else 
        System.out.print("yes");
    }
}