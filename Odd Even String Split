import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        String userInput = obj.next();
        System.out.print(userInput.charAt(0));
        for(int i=2;i<userInput.length();i+=2){
            System.out.print(userInput.charAt(i));
        }
        System.out.print(" ");
        for(int i=1;i<userInput.length();i+=2){
            System.out.print(userInput.charAt(i));
        }
    }
}