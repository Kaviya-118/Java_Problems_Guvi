import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int userInput1 = obj.nextInt();
        int userInput2 = obj.nextInt();
        if((userInput1+userInput2)%2==0)
        System.out.println("even");
        else
        System.out.println("odd");
    }
}